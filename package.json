{
  "name": "vite-plugin-arraybuffer",
  "description": "Vite plugin for import file with ArrayBuffer or Uint8Array.",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "dist/types.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "format": "prettier -w",
    "typing": "tsc --noEmit",
    "build": "tsup && cp src/types.d.ts dist/",
    "test": "vitest --run",
    "prepublish": "pnpm test && pnpm build && bumpp"
  },
  "devDependencies": {
    "@types/node": "22.13.5",
    "bumpp": "10.0.3",
    "tsup": "8.4.0",
    "typescript": "5.7.3",
    "vite": "6.2.4",
    "vitest": "3.0.7"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/tachibana-shin/vite-plugin-arraybuffer.git"
  },
  "homepage": "https://tachibana-shin.github.io/vite-plugin-arraybuffer",
  "keywords": [
    "vite-plugin-arraybuffer",
    "Tachibana Shin",
    "vite",
    "arraybuffer",
    "import file",
    "tachibana-shin"
  ]
}
